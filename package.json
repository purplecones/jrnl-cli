    "file-system": "^2.2.2",
    "inquirer": "^3.2.3",
    "markdown-table": "^1.1.1",
    "moment": "^2.18.1",
    "nedb": "^1.8.0",
    "sentiment": "^4.0.1",
    "shelljs": "^0.7.8"
  }
